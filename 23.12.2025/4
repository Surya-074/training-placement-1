def merge(a):
    if len(a)<=1:
        return a,0
    m=len(a)//2
    l,c1=merge(a[:m])
    r,c2=merge(a[m:])
    i=j=0
    c=c1+c2
    res=[]
    while i<len(l) and j<len(r):
        if l[i]<=r[j]:
            res.append(l[i]);i+=1
        else:
            res.append(r[j]);j+=1
            c+=len(l)-i
    return res+l[i:]+r[j:],c

arr=list(map(int,input().split()))
print(merge(arr)[1])
