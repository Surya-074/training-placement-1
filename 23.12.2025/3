s = input()
p = input()
lps = [0]*len(p)
j = 0
for i in range(1,len(p)):
    while j>0 and p[i]!=p[j]:
        j = lps[j-1]
    if p[i]==p[j]:
        j+=1
        lps[i]=j
j = 0
for i in range(len(s)):
    while j>0 and s[i]!=p[j]:
        j = lps[j-1]
    if s[i]==p[j]:
        j+=1
    if j==len(p):
        print(i-j+1)
        break
