public class LinearSearch {
    public static void main(String[] args) {
        int[] arr = {3, 5, 7, 9, 11};
        int key = 7;
        boolean found = false;
        for(int num : arr)
            if(num == key) {
                found = true;
                break;
            }
        System.out.println(found ? "Found" : "Not Found");
    }
}
