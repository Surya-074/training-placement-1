class MathOp {
    int add(int a, int b) {
        return a + b;
    }
    double add(double a, double b) {
        return a + b;
    }
}
public class Main {
    public static void main(String[] args) {
        MathOp m = new MathOp();
        System.out.println(m.add(5, 3));
        System.out.println(m.add(2.5, 3.5));
    }
}
