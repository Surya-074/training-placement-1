s = input()
t = input()
from collections import Counter
ct = Counter(t)
l = 0
res = ""
need = len(t)
for r in range(len(s)):
    if ct[s[r]] > 0:
        need -= 1
    ct[s[r]] -= 1
    while need == 0:
        if not res or r-l+1 < len(res):
            res = s[l:r+1]
        ct[s[l]] += 1
        if ct[s[l]] > 0:
            need += 1
        l += 1
print(res)
